{
  "name": "svelte-starter-kit",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@storybook/addon-actions": "^7.0.0-rc.4",
    "@storybook/addon-essentials": "^7.0.0-rc.4",
    "@storybook/addon-interactions": "^7.0.0-rc.4",
    "@storybook/addon-links": "^7.0.0-rc.4",
    "@storybook/addon-mdx-gfm": "^7.0.0-rc.4",
    "@storybook/svelte": "^7.0.0-rc.4",
    "@storybook/sveltekit": "^7.0.0-rc.4",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-vercel": "2.3.1",
    "@sveltejs/kit": "^1.5.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-rc.4",
    "svelte": "^3.57.0",
    "svelte-check": "^3.0.1",
    "svelte-loader": "^3.1.7",
    "tailwindcss": "^3.2.7",
    "vite": "^4.1.4"
  },
  "type": "module",
  "dependencies": {
    "daisyui": "^2.51.4",
    "i": "^0.3.7",
    "node-fetch": "^2.6.9"
  }
}
